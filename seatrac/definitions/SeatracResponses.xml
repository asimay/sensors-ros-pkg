	</struct>
	
	<struct name="PingResp" inherit="SeatracMessage">
	    <enum name="CID" value="0x42" />
	    <var type="AcoFix" name="acofix" />
	</struct>
	
	<struct name="PingError" inherit="SeatracMessage">
	    <enum name="CID" value="0x43" />
